{
  "name": "casino_os_server",
  "version": "1.0.0",
  "description": "casino os server",
  "main": "server.js",
  "author": "kunkeypr",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --unhandled-rejections=strict src/server.js",
    "deploy": "chmod 777 ./bin/deploy.sh && ./bin/deploy.sh",
    "serve": "node server.js",
    "serve-debug": "nodemon --inspect src/server.js",
    "start": "node --openssl-legacy-provider server.js",
    "test": "jest --forceExit --coverage --verbose",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org vnpt-q6 --project node-express ./dist && sentry-cli sourcemaps upload --org vnpt-q6 --project node-express ./dist"
  },
  "_moduleAliases": {
    "@": "./src",
    "@Connect": "./src/connect",
    "@Express": "./src/express",
    "@SocketIO": "./src/socketIO",
    "@Config": "./src/config",
    "@Configs": "./src/configs",
    "@Controllers": "./src/controllers",
    "@HttpControllers": "./src/controllers/http",
    "@SocketControllers": "./src/controllers/socket",
    "@SocketIoControllers": "./src/controllers/socketIo",
    "@EntryControllers": "./src/controllers/entry",
    "@Databases": "./src/databases",
    "@Helpers": "./src/helpers",
    "@Loaders": "./src/loaders",
    "@Middwares": "./src/middwares",
    "@Models": "./src/models",
    "@Plugins": "./src/plugins",
    "@Routers": "./src/routers/http",
    "@HttpRouters": "./src/routers/http",
    "@SocketRouters": "./src/routers/socket",
    "@SocketIoRouters": "./src/routers/socketIo",
    "@EntryRouters": "./src/routers/entry",
    "@Services": "./src/services",
    "@Utils": "./src/utils"
  },
  "dependencies": {
    "@sentry/cli": "^2.37.0",
    "@sentry/node": "^8.34.0",
    "@sentry/profiling-node": "^8.34.0",
    "assert": "^2.0.0",
    "async-redis": "^2.0.0",
    "axios": "1.1.2",
    "body-parser": "^1.20.1",
    "cluster": "^0.7.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^2.1.0",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-ws": "^5.0.2",
    "ip-location-api": "^2.1.4",
    "ipapi-tools": "^1.0.4",
    "js-sha256": "^0.9.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "module-alias": "^2.2.2",
    "moment-random": "^1.1.0",
    "moment-timezone": "^0.5.39",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.20",
    "qs": "^6.11.0",
    "sequelize": "^6.25.3",
    "sequelize-cli": "^6.6.2",
    "simple-git": "^3.27.0",
    "socket.io": "^4.6.1",
    "svg-captcha": "^1.4.0",
    "svg-to-img": "^2.0.9",
    "validator": "^13.7.0"
  }
}
